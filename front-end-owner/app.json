{
  "expo": {
    "name": "소소행사장님",
    "slug": "sosohang-owner",
    "displayName": "소소행",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/soso_logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/soso_logo.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "config": {
        "googleMaps": {
          "apiKey": ""
        }
      },
      "package": "com.sosohang.sosohang",
      "adaptiveIcon": {
        "foregroundImage": "./assets/soso_logo.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "READ_CONTACTS"
      ]
    },
    "web": {
      "favicon": "./assets/soso_logo.png"
    }
  }
}
